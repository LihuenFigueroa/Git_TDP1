
#include "aten.h"

void Aten_Init(){
	//poner aten en 127
	atenActual=127;

}

void Aten_Write(uint8_t valor){
	if(valor & 10000000){

	}
	else{}

	if(valor & 01000000){

	}
	else{}

	if(valor & 00100000){

	}
	else{}

	if(valor & 00010000){

	}
	else{}

	if(valor & 00001000){

	}
	else{}

	if(valor & 00000100){

	}
	else{}

	if(valor & 00000010){

	}
	else{

	}
}


uint8_t Aten_SetValue (uint8_t valor){
	if(valor>127){
		return 0;
	}else{
		Aten_Write(valor);
	}

	return 1;
}

